-- Define the expected key
local expectedKey = "79ddc587ee8eae6a7d3044e972b3389c"

-- Function to validate the key
function validateKey(providedKey)
    return providedKey == expectedKey
end

-- Example usage:
local providedKey = "79ddc587ee8eae6a7d3044e972b3389c" -- Replace with the actual key provided
if validateKey(providedKey) then
    print("Key is valid, continue execution")
else
    print("Key is invalid, execution stopped")
    return -- You can stop script execution or add additional logic here
end

